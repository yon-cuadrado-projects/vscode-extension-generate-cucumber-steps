{
	"name": "generate-cucumber-steps",
	"displayName": "generate-cucumber-steps",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:generate-cucumber-steps.generate-and-copy-to-clipboard"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "generate-cucumber-steps.generate-and-copy-to-clipboard",
				"title": "generate-and-copy-to-clipboard"
			}
		],
		"keybindings": [
			{
				"command": "generate-cucumber-steps.generate-and-copy-to-clipboard",
				"key": "ctrl+shift+c",
				"mac": "ctrl+shift+c",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"title": "generate-cucumber-steps",
			"properties": {
				"generate-cucumber-steps.stepFunctionType": {
					"type": "string",
					"scope": "application",
					"default": "arrow function",
					"pattern": "arrow function|function expression",
					"description": "It defines the type of function of the step: function expression or arrow function"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.19",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/should": "^13.0.0",
		"@types/sinon-chai": "^3.2.5",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"chai": "^4.3.4",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"sinon": "^11.1.1",
		"sinon-chai": "^3.7.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {}
}
